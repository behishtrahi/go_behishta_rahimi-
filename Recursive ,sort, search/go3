// P2 Q1


package main

import (
	"fmt"
)

func main() {
	fmt.Println(catalan(7))  // 429
	fmt.Println(catalan(10)) // 16796
}

func catalan(n int) int {
	catalan := make([]int, n+1)

	catalan[0] = 1
	catalan[1] = 1

	for i := 2; i <= n; i++ {
		catalan[i] = 0
		for j := 0; j < i; j++ {
			catalan[i] += catalan[j] * catalan[i-j-1]
		}
	}

	return catalan[n]
}
