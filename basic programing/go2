package main

import "fmt"

func main() {

	// Q2 answer
	score := 100

	if score <= 100 && score >= 80 {
		fmt.Println("A")
	} else if score <= 70 && score >= 84 {
		fmt.Println("B")
	}
	if score >= 55 && score <= 69 {
		fmt.Println("C")
	} else if score >= 40 && score <= 50 {
		fmt.Println("D")
	}
	if score >= 0 && score <= 39 {
		fmt.Println("E")
	} else if score < 0 && score > 100 {
		fmt.Println("invalid")
	}
}
