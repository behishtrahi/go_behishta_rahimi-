//P1Q2

package main

// Function to check if a number is prime
func isPrime(n int) bool {
	if n <= 1 {
		return false
	}
	for i := 2; i*i <= n; i++ {
		if n%i == 0 {
			return false
		}
	}
	return true
}

// Function to generate prime numbers and send them to a channel
func generatePrimes(ch chan<- int) {
	for i := 2; i <= 100; i++ {
		if isPrime(i) {
			ch <- i
		}
	}
	close(ch)
}

